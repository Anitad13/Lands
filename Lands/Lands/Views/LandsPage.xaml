    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Lands.Views.LandsPage"
    BindingContext="{Binding Main , Source={StaticResource Locator}}"
   Title="Lands">
    <ContentPage.Content>
          <ScrollView
           BindingContext="{Binding Lands}">
